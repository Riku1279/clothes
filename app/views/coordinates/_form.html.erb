<div class="row">
  <div class="col-6">

<%= form_with(model: coordinate,local:true) do |f| %>

  <div class="form-group">
    <%= f.label :content, '詳細' %>

    <%= f.text_field :content, class: 'form-control' %>
  </div>
  
  <div class="form-group">
  
    <%= f.label :img, '画像' %>
  
    <%= f.file_field :img %>
  </div>
  
  <%= f.submit '投稿', class: 'btn btn-success' %>
<% end %>

</div>
  </div>
