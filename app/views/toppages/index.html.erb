<% if logged_in? %>
  <h1 class='text-center text-warning'><i class="fas fa-tshirt"></i>users coordinate</h1>
  
  <%= render 'coordinates/coordinates', coordinates: @coordinates %>
  
<% else %>  
  <div class=' text-center '>
    <h1 class= 'text-warning '>Lets be fashionable!</h1>
 
    <%= link_to 'Sign up ', signup_path, class: 'btn btn-primary' %>
    <h2>or</h2>
    <%= link_to 'Login',login_path,class: 'btn btn-success' %>
  </div>
<% end %>